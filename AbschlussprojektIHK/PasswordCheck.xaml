        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AbschlussprojektIHK"
        mc:Ignorable="d"
        Title="PasswordCheck" Height="450" Width="800">
    <Grid>
        <PasswordBox x:Name="Pwb_Password" HorizontalAlignment="Left" Margin="160,197,0,0" VerticalAlignment="Top" Width="149"/>
        <Label Content="Label" HorizontalAlignment="Left" Margin="160,166,0,0" VerticalAlignment="Top"/>
        <Button x:Name="Btn_Abort" Content="Abbrechen" HorizontalAlignment="Left" Margin="102,267,0,0" VerticalAlignment="Top" Width="75" Click="Btn_Abort_Click"/>
        <Button x:Name="Btn_Continue" Content="Ok" HorizontalAlignment="Left" Margin="195,267,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0,-0.664" Click="Btn_Continue_ClickAsync"/>

    </Grid>
